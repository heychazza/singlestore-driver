{
    "name": "heychazza/singlestore-driver",
    "description": "My package",
    "keywords": [
        "heychazza",
        "laravel",
        "singlestore-driver"
    ],
    "homepage": "https://github.com/heychazza/singlestore-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "heychazza",
            "email": "ch@rli.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "doctrine/dbal": "^2.6|^3.0",
        "illuminate/contracts": "dev-master",
        "illuminate/database": "dev-master",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Heychazza\\SingleStoreDriver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heychazza\\SingleStoreDriver\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Heychazza\\SingleStoreDriver\\SingleStoreDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
